{
    "name": "server-manager-ui",
    "version": "1.3.0",
    "scripts": {
        "build": "npm run ng -- build --prod --build-optimizer=true --statsJson=true && npm run build:version",
        "build:debug": "npm run ng -- build --prod --source-map --build-optimizer=true --statsJson=true && npm run build:version",
        "build:version": "node scripts/version.js",
        "bundle-report": "webpack-bundle-analyzer ../dist/ui/stats-es2015.json",
        "e2e": "npm run ng -- e2e",
        "generate:component": "./node_modules/.bin/ng generate @sbpro/ng:component",
        "generate:directive": "./node_modules/.bin/ng generate @sbpro/ng:directive",
        "generate:module": "./node_modules/.bin/ng generate @sbpro/ng:module",
        "generate:service": "./node_modules/.bin/ng generate @sbpro/ng:service",
        "lint:fix": "npm run ng -- lint --fix",
        "lint": "npm run ng -- lint",
        "ng": "npx cross-env NODE_OPTIONS=--max_old_space_size=2048 ./node_modules/.bin/ng",
        "serve": "./node_modules/.bin/static-server ../dist/ui",
        "start": "node scripts/start.js",
        "test": "npm run ng -- test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "11.2.14",
        "@angular/cdk": "11.2.13",
        "@angular/common": "11.2.14",
        "@angular/compiler": "11.2.14",
        "@angular/core": "11.2.14",
        "@angular/forms": "11.2.14",
        "@angular/material": "^11.0.1",
        "@angular/platform-browser": "11.2.14",
        "@angular/platform-browser-dynamic": "11.2.14",
        "@angular/router": "11.2.14",
        "@asymmetrik/ngx-leaflet": "^8.1.0",
        "@fortawesome/angular-fontawesome": "0.8.0",
        "@fortawesome/fontawesome-svg-core": "6.2.1",
        "@fortawesome/free-brands-svg-icons": "6.2.1",
        "@fortawesome/free-regular-svg-icons": "6.2.1",
        "@fortawesome/free-solid-svg-icons": "6.2.1",
        "@ng-bootstrap/ng-bootstrap": "8.0.4",
        "@ng-select/ng-select": "6.1.0",
        "ag-grid-angular": "25.3.0",
        "ag-grid-community": "25.3.0",
        "chart.js": "2.9.4",
        "leaflet": "1.9.4",
        "ngx-gauge": "3.0.0",
        "object-hash": "3.0.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.7",
        "stream": "0.0.2",
        "timers": "0.1.1",
        "tslib": "2.6.0",
        "uuid": "9.0.0",
        "webpack-bundle-analyzer": "4.9.0",
        "xml2js": "0.6.0",
        "zone.js": "0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1102.19",
        "@angular/cli": "^11.2.7",
        "@angular/compiler-cli": "11.2.14",
        "@angular/language-service": "11.2.14",
        "@angular/localize": "^11.0.1",
        "@inip/static-server": "1.0.1",
        "@sbpro/ng": "3.1.0",
        "@types/chart.js": "2.9.37",
        "@types/jasmine": "4.3.5",
        "@types/jasminewd2": "2.0.10",
        "@types/leaflet": "^1.7.0",
        "@types/node": "18.14.0",
        "@types/object-hash": "3.0.2",
        "@types/uuid": "9.0.2",
        "@types/xml2js": "0.4.11",
        "bootstrap": "5.3.0",
        "chokidar": "3.5.3",
        "codelyzer": "6.0.2",
        "concurrently": "8.2.0",
        "cross-env": "7.0.3",
        "jasmine-core": "5.1.0",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.2",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "prettier": "3.0.0",
        "protractor": "7.0.0",
        "shelljs": "0.8.5",
        "ts-node": "10.9.1",
        "tslint": "6.1.3",
        "tslint-plugin-prettier": "2.3.0",
        "typescript": "5.1.6"
    }
}
